switch(val) {
  case 1:
    result = "alpha";
    break;
  case 2:
    result = "beta";
    break;
  case 3:
    result = "gamma";
    break;
  case 4:
    result = "delta";
    break;
}

// Object lookup
const lookup = {
  1: "alpha",
  2: "beta",
  3: "gamma",
  4: "delta"
};
result = lookup[val];



