  method: "POST",
  headers: {
    "Content-Type": "application/json",
    "Authorization": "Bearer tu-api-key"
  },
  body: JSON.stringify({
    "model": "text-davinci-003",
    "prompt": "Hazme un archivo html para una app que pida un prompt y me muestre la respuesta",
    "max_tokens": 3000,
    "temperature": 0
  })
})
